.meng {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  position: fixed;
  top: 0rpx;
  left: 0rpx;
}

.mengbox {
  width: 566rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  margin-left: -283rpx;
  margin-top: -220rpx;
  background: #fff;
  border-radius: 12rpx;
  padding: 65rpx 48rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
}

.mengtitle {
  font-size: 30rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(51, 62, 82, 1);
  margin-bottom: 89rpx;
}

.mengbom {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  position: absolute;
  width: 100%;
  bottom: 0rpx;
  left: 0rpx;
  padding-bottom: 30rpx;
}

.cancle {
  font-size: 28rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(131, 145, 168, 1);
}

.sub {
  font-size: 28rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(90, 175, 224, 1);
}
