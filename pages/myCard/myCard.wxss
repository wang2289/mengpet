.myInfo {
  height: 180rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 16rpx;
  display: flex;
  position: relative;
}
.peo-icon {
  width: 20rpx;
  height: 20rpx;
  margin: 0 5rpx;
}
.peopleimage {
  width: 120rpx;
  height: 120rpx;
  margin-top: 20rpx;
  margin-left: 30rpx;
  display: inline-block;
}
.peopleinfo {
  display: flex;
  display: inline-block;
  flex-direction: column;
  margin-top: 32rpx;
  margin-left: 30rpx;
  width: 500rpx;
}
.peoplename {
  font-size: 34rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(51, 62, 82, 1);
  display: block;
  margin-left:30rpx;
}
.peopleid {
  font-size: 26rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(131, 145, 168, 1);
  display: block;
  margin-top: 12rpx;
}
.container {
  background-color: white;
  width: 100%;
  position: relative;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.tips {
  width: 100%;
  height: 62rpx;
  background: rgba(240, 207, 189, 0.3);
  font-size: 22rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(51, 62, 82, 1);
  line-height: 62rpx;
  padding-left: 30rpx;
  box-sizing: border-box;
}
.peopleInfoForm {
  display: flex;
  flex-direction: column;
  width: 100%;
  padding: 30rpx;
}
.genderItem {
  display: flex;
}
.sub-btn {
  bottom: 0;
  position: fixed;
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: #fff;
  width: 100%;
  height: 120rpx;
  box-sizing: border-box;
  padding: 16rpx 20rpx;
  z-index: 999;
}
.btns {
  width: 690rpx;
  height: 88rpx;
  line-height: 88rpx;
  text-align: center;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: rgba(255,255,255,1);
  line-height: 96rpx;
  background: rgba(90,175,224,1);
  border-radius: 16rpx;
}
.myCell {
  position: relative;
  width: 100%;
  padding-top: 0rpx;
  box-sizing: border-box;
}
.line {
  width: 690rpx;
  margin: auto;
  height: 1rpx;margin-top: 1rpx;
  margin-bottom: 1rpx;
  background: #E6EAEF;
  transform: rotateZ(360deg);
}
.own-info-item {
  display: flex;
  flex-direction: row;
  width: 100%;
  padding: 15rpx 30rpx;
}
.valueClass {
  text-align: left !important;
}
.titleClass {
  margin-left: 30rpx;
}