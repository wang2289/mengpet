.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  width: 100%;
}

.apply {
  width: 400rpx;
  height: 310rpx;
  margin-top: 100rpx;
  margin-left: 50rpx;
}

.confirm {
  font-size: 40rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: rgba(51, 62, 82, 1);
}

.subtitle {
  font-size: 28rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(131, 145, 168, 1);
  padding: 30rpx 60rpx;
  padding-bottom: 120rpx;
  text-align: center;
}

.cankao {
  font-size: 26rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(131, 145, 168, 1);
}

.cankaotitle {
  font-size: 26rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(90, 175, 224, 1);
  text-decoration: underline;
}

.sub-btn {
  bottom: 0;
  position: fixed;
  display: flex;
  flex-direction: column;
  align-items: center;
  /* width:690rpx; */
  background-color: #fff;
  width: 100%;
  height: 120rpx;
  box-sizing: border-box;
  padding: 16rpx 20rpx;
  z-index: 999;
}

.btns {
  width: 690rpx;
  height: 88rpx;
  line-height: 88rpx;
  text-align: center;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
  line-height: 48px;
  background: rgba(90, 175, 224, 1);
  border-radius: 16rpx;
}
